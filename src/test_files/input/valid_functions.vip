'''Declaración y uso de Funciones'''

def float square(float x): {
    return x * x
}

def float power(float f; int i): {
    float r = 1.0
    int s = 1
    while s < i: {
        r = r * f
        s = s + 1
    }
    return r
}



# también se puede devolver un registro en una función

float result_2 = power(2.0, 30)

float[5] float_vector

